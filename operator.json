{
  "name": "Random Forest",
  "description": "Fits a Random Forest model to your data.",
  "tags": [
    "machine learning"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/rfsrc_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "random_seed",
      "defaultValue": -1,
      "description": "Random seed for reproducible results."
    },
    {
      "kind": "DoubleProperty",
      "name": "n_trees",
      "defaultValue": 500,
      "description": "Number of trees."
    },
    {
      "kind": "EnumeratedProperty",
      "name": "importance",
      "defaultValue": "anti",
      "values": [
        "anti",
        "permute",
        "random"
      ],
      "description": "Method for computing variable importance. See rfsrc documentation."
    },
    {
      "kind": "BooleanProperty",
      "name": "find_interactions",
      "defaultValue": false,
      "description": "Compute importance of variable interactions."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "1.0.0",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Variables/features for the model",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Observations/samples",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Y-Axis",
                "type": "numeric",
                "description": "Measurement values",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "Label",
                "type": "",
                "description": "Target variable for prediction (required)",
                "ontologyMapping": "group",
                "crosstabMapping": "label",
                "cardinality": "1..n",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "ConditionalJoinSpec",
        "description": "Output varies based on find_interactions property",
        "alternatives": [
          {
            "kind": "OutputAlternative",
            "condition": "Without interactions (find_interactions = false)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Row"
                    ],
                    "rColumns": [
                      "Row"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "importance",
                        "type": "double"
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "kind": "OutputAlternative",
            "condition": "With interactions (find_interactions = true)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Row"
                    ],
                    "rColumns": [
                      "Row"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "importance",
                        "type": "double"
                      }
                    ]
                  }
                },
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Row"
                    ],
                    "rColumns": [
                      "Row"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "Multivar.1",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "interaction",
                        "type": "string"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ]
  }
}