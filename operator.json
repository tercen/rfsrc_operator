{
  "name": "Random Forest",
  "description": "Fits a Random Forest model to your data.",
  "tags": [
    "machine learning"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/rfsrc_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "random_seed",
      "defaultValue": -1,
      "description": "Random seed for reproducible results."
    },
    {
      "kind": "DoubleProperty",
      "name": "n_trees",
      "defaultValue": 500,
      "description": "Number of trees."
    },
    {
      "kind": "EnumeratedProperty",
      "name": "importance",
      "defaultValue": "anti",
      "values": [
        "anti",
        "permute",
        "random"
      ],
      "description": "Method for computing variable importance. See rfsrc documentation."
    },
    {
      "kind": "BooleanProperty",
      "name": "find_interactions",
      "defaultValue": false,
      "description": "Compute importance of variable interactions."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Labels",
            "type": "",
            "description": "Indicates the groups to compare.",
            "ontologyMapping": "label",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Variables",
            "type": "",
            "description": "Indicates the variables (e.g., genes, markers).",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Observations",
            "type": "",
            "description": "Indicates the observations (e.g., samples, cells, individuals).",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement value.",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}